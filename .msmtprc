# -*- conf-space -*-
# Skeleton ~/.msmtprc

defaults
auth                    on
host                    smtp.bar.baz
logfile                 ~/.log/msmtp
port                    465
tls                     on
tls_certcheck           on
tls_starttls            off
tls_trust_file          /etc/ssl/certs/ca-certificates.crt

account                 foo
from                    foo@bar.baz
user                    foo@bar.baz
passwordeval            "pass foo/bar"
