# -*- conf-space -*-
# Skeleton ~/.msmtprc

defaults
auth                on
logfile             ~/.cache/msmtp/msmtp.log
port                465
tls                 on
tls_starttls        off

account             foo
from                foo@bar.baz
host                smtp.bar.baz
passwordeval        "pass foo/bar"
user                foo@bar.baz

account             bar
from                foo@bar.baz
host                smtp.bar.baz
passwordeval        "pass foo/bar"
port                587
tls_starttls        on
user                foo@bar.baz
